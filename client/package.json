    "react-avatar-editor": "^11.0.4",
    "react-chartist": "^0.13.1",
    "react-dom": "^16.2.0",
    "react-dropzone": "^5.1.0",
    "react-easy-chart": "^1.0.0",
    "react-ga": "^2.5.3",
    "react-moment": "^0.6.8",
    "react-redux": "^5.0.6",
    "react-router": "^4.2.0",
